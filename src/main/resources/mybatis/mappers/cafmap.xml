<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cafmap.dao.MemDao">

    <select id="login" parameterType="hashmap" resultType="com.cafmap.dto.MemDto">
        select mem_pwd from mvc_member where mem_uid = #{mem_uid}
    </select>
    
    <insert id="write" parameterType="hashmap">
   		insert into mvc_member values(#{mem_uid},#{mem_pwd},#{mem_name})
    </insert>

</mapper>